{
    "name": "highcharts-svelte",
    "description": "A simple & intuitive Svelte wrapper for Highcharts",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "svelte-package",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "prepare": "husky"
    },
    "keywords": [
        "highcharts",
        "svelte",
        "wrapper",
        "chart",
        "graph",
        "visualization"
    ],
    "author": "Jakub Szumi≈Ñski <jakubszuminski0@gmail.com>",
    "repository": "https://github.com/highcharts/highcharts-svelte",
    "bugs": {
        "url": "https://github.com/highcharts/highcharts-vue/issues"
    },
    "license": "SEE LICENSE IN LICENSE",
    "lint-staged": {
        "{.js,.ts,.svelte}": "eslint"
    },
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "devDependencies": {
        "@sveltejs/package": "^2.3.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tsconfig/svelte": "^5.0.2",
        "@types/node": "^20.11.21",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-plugin-svelte": "^2.35.1",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "svelte": "^4.2.11",
        "svelte-check": "^3.6.4",
        "svelte-kit": "^1.2.0",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^5.1.4"
    },
    "peerDependencies": {
        "highcharts": ">5.0.0",
        "svelte": ">=3.0.0 <5.0.0"
    }
}
